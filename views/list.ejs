<% layout('layout') -%>
<link href="/css/style.css" rel="stylesheet" type="text/css">
<ul class="poke-list">
    <% for (var i = 0; i < pokeNbr ; i++) { %> 
        <li class="poke-case"><img src="/image/<%= pokeList[i].id %>.png" alt="<%= pokeList[i].name %>">
        <a href="/pokemon/<%= pokeList[i].id %>"><%= pokeList[i].name %> #<%= pokeList[i].id %></a></li>
    <% } %>
</ul>